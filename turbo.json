{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".svelte-kit/**"]
      },
      "deploy": {
        "dependsOn": ["build", "test", "lint"]
      },
      "test": {},
      "lint": {},
      "dev": {
          "cache": false,
          "persistent": true
      }
    }
  }